<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp">

    <com.br.shortener_link.feature.shortUrl.presentation.view.ShortUrlView
        android:id="@+id/shortUrlView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/view_short_url" />

    <com.br.shortener_link.feature.shortUrl.presentation.view.ShortUrlListView
        android:id="@+id/shortUrlListView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/shortUrlView"
        tools:layout="@layout/view_short_url_list" />

</androidx.constraintlayout.widget.ConstraintLayout>